<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nihaoyin.healthmanagement.dao.MenuDao">
    <select id="getMenu" resultType="com.nihaoyin.healthmanagement.bean.User">
        SELECT id FROM sportplay.user
        WHERE username=#{username} AND password=#{password}
    </select>
</mapper>